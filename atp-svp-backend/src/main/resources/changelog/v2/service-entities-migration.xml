<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <changeSet id="send_service_entities_to_kafka" author="atp-svp-dev-v2">
        <preConditions onFail="CONTINUE">
            <changeLogPropertyDefined property="service.entities.migration.enabled" value="true"/>
        </preConditions>
        <comment>Sending service entities to Kafka topic.</comment>
        <customChange class="org.qubership.atp.svp.migration.v2.ServiceEntitiesMigrationCustomChange">
            <param name="serviceName" value="${spring.application.name}"/>
        </customChange>
    </changeSet>

</databaseChangeLog>
